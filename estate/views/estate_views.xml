<?xml version="1.0"?>
<odoo>
    <record id="estate_action" model="ir.actions.act_window">
        <field name="name">Estate Action</field>
        <field name="res_model">estate</field>
        <field name="view_mode">list,form</field>
    </record>

    <record id="estate_view_tree" model="ir.ui.view">
        <field name="name">estate.list</field>
        <field name="model">estate</field>
        <field name="arch" type="xml">
            <list string="Channel">
                <field name="name">Name</field>
                <field name="postcode">Postcode</field>
                <field name="bedrooms">Bedrooms</field>
                <field name="living_area">Living Area (sqm)</field>
                <field name="expected_price">Expected Price</field>
                <field name="selling_price">Selling Price</field>
                <field name="date_availability">Available From</field>
            </list>
        </field>
    </record>
</odoo>